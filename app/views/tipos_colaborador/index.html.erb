<p id="notice"><%= notice %></p>

<h1>Tipos Colaborador</h1>

<table>
  <thead>
    <tr>
      <th>Descricao</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipos_colaborador.each do |tipo_colaborador| %>
      <tr>
        <td><%= tipo_colaborador.descricao %></td>
        <td><%= link_to 'Show', tipo_colaborador %></td>
        <td><%= link_to 'Edit', edit_tipo_colaborador_path(tipo_colaborador) %></td>
        <td><%= link_to 'Destroy', tipo_colaborador, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo Colaborador', new_tipo_colaborador_path %>
